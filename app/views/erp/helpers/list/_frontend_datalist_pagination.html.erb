<!-- Pagination -->
<div class="inner-pagination pagination m-none">
    <nav aria-label="Page navigation example">
        <%= will_paginate data, previous_label: '<i class="fa fa-angle-double-left"></i>'.html_safe, next_label: '<i class="fa fa-angle-double-right"></i>'.html_safe, renderer: BootstrapPagination::Rails %>
    </nav>
</div>

<div class="inner-pagination pagination m-none">
    <nav aria-label="Page navigation example">
        <ul class="pagination m-none">
            <li class="page-item">
                <a class="page-link" href="#" aria-label="Previous">
                    <span aria-hidden="true">&laquo;</span>
                    <span class="sr-only">Previous</span>
                </a>
            </li>
            <li class="page-item"><a class="page-link" href="#">1</a></li>
            <li class="page-item"><a class="page-link" href="#">2</a></li>
            <li class="page-item"><a class="page-link" href="#">3</a></li>
            <li class="page-item">
                <a class="page-link" href="#" aria-label="Next">
                    <span aria-hidden="true">&raquo;</span>
                    <span class="sr-only">Next</span>
                </a>
            </li>
        </ul>
    </nav>
</div>