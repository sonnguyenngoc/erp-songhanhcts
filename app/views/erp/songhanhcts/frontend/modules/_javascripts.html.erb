<script>
  (function($){
    $(document).ready(function(){
      $('ul.dropdown-menu [data-toggle=dropdown-submenu]').on('click', function(event) {
        event.preventDefault(); 
        event.stopPropagation(); 
        $(this).parent().siblings().removeClass('open');
        $(this).parent().toggleClass('open');
      });
    });
  })(jQuery);
  
  $(document).ready(function () {
      $('#media').carousel({
          pause: true,
          interval: 5200,
      });
  });
</script>